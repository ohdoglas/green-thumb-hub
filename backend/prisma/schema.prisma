
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  user_id Int @id @default(autoincrement())
  email String @unique
  password String
  username String @unique
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}


